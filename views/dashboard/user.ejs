<!DOCTYPE html>
<html lang="en">

<head>
  <%- include ../templates/header.ejs %>
</head>

<body class="">
  <div id="app">
    <div class="main-wrapper">
      <%- include ../templates/navtop.ejs %>
        <%- include ../templates/navleft.ejs %>
          <div class="main-content">
            <section class="section">
              <div id="carouselExampleIndicators2" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators" id="promo-num">
                </ol>
                <div class="carousel-inner" id="promo-gambar">
                </div>
                <a class="carousel-control-prev" href="#carouselExampleIndicators2" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleIndicators2" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>
              <div class="section-header">
                <h1>Dashboard</h1>
                <div class="section-header-breadcrumb">
                  <div class="breadcrumb-item active">
                    <a href="#">Dashboard</a>
                  </div>
                  <div class="breadcrumb-item">Default Layout</div>
                </div>
              </div>
              <div class="section-body">
                <h2 class="section-title">Hi, <%- auth.nama %> !</h2>
                <p class="section-lead">
                  Change information about yourself on this page.
                </p>

                <div class="row mt-sm-4">
                  <div class="col-12 col-md-12 col-lg-8">
                    <div class="card profile-widget">
                      <div class="profile-widget-header">
                        <img alt="image" src="<%= process.env.URL %>/assets/img/avatar/avatar-1.png"
                          class="rounded-circle profile-widget-picture" />
                        <div class="profile-widget-items">
                          <div class="profile-widget-item">
                            <div class="profile-widget-item-label">Cars</div>
                            <div class="profile-widget-item-value"><span id="jmlcars"></span></div>
                          </div>
                          <div class="profile-widget-item">
                            <div class="profile-widget-item-label">Point</div>
                            <div class="profile-widget-item-value"><span id="pointcars"></span></div>
                          </div>
                        </div>
                      </div>
                      <div class="profile-widget-description">
                        <div class="profile-widget-name">
                          <%- auth.nama %>
                            <div class="text-muted d-inline font-weight-normal">
                              <div class="slash"></div>
                              Cendrawasih
                            </div>
                        </div>
                        <div class="text-left">
                          <a href="https://wa.me/6287855925696" target="_blank"  class="btn btn-success">
                            <i class="fab fa-whatsapp"></i> Chat Me!
                          </a>
                          <a href="https://wa.me/628123811077" target="_blank" class="btn btn-danger">
                            <i class="fas fa-heartbeat"></i> Emergency Call
                          </a>
                          <a href="/enews" class="btn btn-primary"><i class="fas fa-newspaper"></i> E-News</a>
                        </div>
                      </div>
                    </div>
                    <div class="card">
                      <div class="card gradient-bottom">
                        <div class="card-header">
                          <h4>Dealer Information</h4>
                        </div>
                        <div class="card-body">
                          <p>
                            Agung Toyota Kuta<br />
                            Jl. Sunsetroad, Seminyak Kuta Badung<br />
                            CALL : (0361) 8947407 - 8947409<br />
                          </p>
                          <iframe
                            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3943.955541462354!2d115.17393451438686!3d-8.695772090990724!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x2dd246d9e4c4ffab%3A0x5dc2279969e5621d!2sAgung%20Toyota%20Kuta!5e0!3m2!1sid!2sid!4v1624874651612!5m2!1sid!2sid"
                            width="100%" height="150" style="border: 0" allowfullscreen="" loading="lazy"></iframe>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-12 col-md-12 col-lg-4">
                    <div class="card">
                      <div class="card gradient-bottom">
                        <div class="card-header">
                          <h4>My Cars</h4>
                        </div>
                        <div class="card-body" id="top-5-scroll">
                          <ul id="listcars" class="list-unstyled list-unstyled-border">
                          </ul>
                        </div>
                        <div class="card-footer pt-3 d-flex justify-content-center">
                          <div class="budget-price justify-content-center">
                            <div class="budget-price-square bg-warning" data-width="20"></div>
                            <div class="budget-price-label">Segera Booking</div>
                          </div>
                          <div class="budget-price justify-content-center">
                            <div class="budget-price-square bg-danger" data-width="20"></div>
                            <div class="budget-price-label">
                              Sudah Jatuh Tempo
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </section>

            <!-- modal -->
            <div class="modal fade" tabindex="-1" role="dialog" id="exampleModal">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title">Detail Kendaraan</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <form action="">
                      <div class="form-group">
                        <label>Nama Customer</label>
                        <input type="text" class="form-control" />
                      </div>
                      <div class="form-group">
                        <label>Telepon (62 Format)</label>
                        <input type="text" class="form-control phone-number" />
                      </div>
                      <div class="form-group">
                        <label>Alamat</label>
                        <textarea name="" id="" class="form-control"></textarea>
                      </div>
                      <div class="form-group">
                        <label>Status</label>
                        <select class="form-control">
                          <option value="1">Aktif</option>
                          <option value="0">Tidak Aktif</option>
                        </select>
                      </div>
                    </form>
                  </div>
                  <div class="modal-footer bg-whitesmoke br">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">
                      Close
                    </button>
                    <button type="button" class="btn btn-primary">
                      Save changes
                    </button>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <%- include ../templates/footer.ejs %>
    </div>
  </div>
  <%- include ../templates/script.ejs %>
  <%- include ../templates/script_d_user.ejs %>
</body>

</html>